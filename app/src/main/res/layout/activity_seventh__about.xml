<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.zeus.ak95.tictactoe.Seventh_About"
    android:background="@drawable/i514"
    android:orientation="vertical"
    >
    <LinearLayout
        android:layout_width="fill_parent" android:id="@+id/svallm1"
        android:layout_height="wrap_content" android:layout_weight="10"
        android:orientation="horizontal" android:background="@drawable/back05">
        <ImageView
            android:layout_width="fill_parent" android:id="@+id/svaiv1"
            android:layout_height="wrap_content" android:layout_centerHorizontal="true"
            android:src="@drawable/tictoe" android:layout_alignParentTop="true" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent" android:id="@+id/svallm2"
        android:layout_height="wrap_content" android:layout_weight="12"
        android:orientation="horizontal" android:background="@drawable/back05"
        android:layout_marginTop="10dp" android:gravity="end|top">

        <TextView
            android:layout_width="fill_parent" android:id="@+id/svatv1"
            android:layout_height="wrap_content" android:textSize="20sp"
            android:gravity="start"
            android:text="About Developer :-"
            android:layout_marginTop="20dp"
            android:textColor="#FFFFFF"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent" android:id="@+id/svallm3"
        android:layout_height="wrap_content" android:layout_weight="2.5"
        android:orientation="vertical" android:background="@drawable/back05"
        android:gravity="center_horizontal" android:layout_marginTop="15dp"
        >

  <EditText
      android:layout_width="fill_parent" android:layout_marginTop="15dp"
      android:layout_height="fill_parent" android:background="#d1FFFFFF"
      android:textSize="17dp" android:textColor="#429773"
      android:verticalScrollbarPosition="right" android:scrollbarFadeDuration="500"
      android:layout_marginBottom="12dp" android:elevation="4dp"
      android:layout_marginLeft="5dp"
      android:scrollbarStyle="insideOverlay"
      android:text="
      This App is designed by me.
      About Content will be
      uploaded soon.
      "

      />
    </LinearLayout>
</LinearLayout>
